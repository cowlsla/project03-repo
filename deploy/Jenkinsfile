pipeline {
  agent any
  tools {
    maven "M3"
    jdk "JDK11"
  }
    
  stages {
    stage ('Git fatch') {
      steps {
       echo 'Git fatch'
       git url: 'https://github.com/cowlsla/spring-petclinic.git',
         branch: 'main', credentialsId: 'gitCredential'
      }
    }
    stage ('Mvn Build') {
      steps {
        echo 'Mvn Build'
        sh 'mvn -Dmaven.test.failure.ignore=true clean package'
      }
    }
    stage ('Docker Image') {
      steps {
       echo 'Docker Image' 
      }
    }
    stage ('Image Push') {
      steps {
       echo 'Image Push' 
      }
    }
    stage ('--') {
      steps {
       echo 'Image Push' 
      }
    }
  }
}
